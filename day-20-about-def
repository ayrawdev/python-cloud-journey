def maths(a,b):
  calculate=((a*b)+(a/b))
  print(calculate)
def islessthan(a,b):
  if(a<b):
    print("a is less than b")
  elif(a==b):
    print("a is equal to b")
  else:
    print("b is greater than a")
a=int(input("Enter the value of a:"))
b=int(input("Enter the value of b:"))
maths(a,b)
islessthan(a,b)
